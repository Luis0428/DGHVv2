        android:background="@drawable/et_personalizado"
        android:ems="10"
        android:inputType="numberSigned"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:textColor="#fff"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView48"
        app:layout_constraintTop_toBottomOf="@+id/textView50" />

    <EditText
        android:id="@+id/editText15"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_below="@+id/editText2"
        android:layout_marginStart="6dp"
        android:layout_marginTop="16dp"
        android:layout_toEndOf="@+id/radioGroup2"
        android:background="@drawable/et_personalizado"
        android:ems="10"
        android:inputType="numberSigned"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:textColor="#fff"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/radioGroup2"
        app:layout_constraintTop_toBottomOf="@+id/editText2"
        android:autofillHints="" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView48"
        android:layout_marginStart="22dp"
        android:layout_marginTop="12dp"
        android:layout_toEndOf="@+id/editText15"
        android:text="@string/comb_P"
        android:textColor="#000"
        android:textSize="36sp"
        app:layout_constraintStart_toEndOf="@+id/editText15"
        app:layout_constraintTop_toBottomOf="@+id/textView48" />

    <EditText
        android:id="@+id/editText17"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_below="@+id/editText16"
        android:layout_marginStart="22dp"
        android:layout_marginTop="16dp"
        android:layout_toEndOf="@+id/textView51"
        android:autofillHints=""
        android:background="@drawable/et_personalizado"
        android:ems="10"
        android:inputType="numberSigned"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:textColor="#fff"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView51"
        app:layout_constraintTop_toBottomOf="@+id/editText16" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="297dp"
        android:layout_height="52dp"
        android:layout_below="@+id/textView51"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:text="@string/comb_res"
        android:textColor="#000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView51" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_below="@+id/textView49"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="87dp"
        android:background="@drawable/tv_personalizado"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView49" />

    <Button
        android:id="@+id/button10"
        android:layout_width="112dp"
        android:layout_height="50dp"
        android:layout_below="@+id/textView31"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="39dp"
        android:background="@drawable/btn_personalizado"
        android:text="@string/btn_resolver"
        android:textColor="#fff"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

    <Button
        android:id="@+id/button9"
        android:layout_width="112dp"
        android:layout_height="50dp"
        android:layout_below="@+id/textView31"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="39dp"
        android:layout_toEndOf="@+id/button10"
        android:background="@drawable/btn_personalizado"
        android:text="@string/btn_limpiar"
        android:textColor="#fff"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/button10"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />
</RelativeLayout>